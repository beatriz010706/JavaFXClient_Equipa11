<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="600.0"
      spacing="10"
      xmlns="http://javafx.com/javafx/25"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="lp.javafxclient_equipa11.controllers.MainController">

   <children>
      <Button text="Menu Estudante"
              mnemonicParsing="false"
              onAction="#abrirMenuEstudante"/>

      <Button text="Menu Parceiro"
              mnemonicParsing="false"
              onAction="#abrirMenuParceiro"/>

      <Button text="Programas"
              mnemonicParsing="false"
              onAction="#abrirMenuProgramas"/>

      <Button text="Candidaturas"
              mnemonicParsing="false"
              onAction="#abrirMenuCandidaturas"/>

      <Button text="Programa Estudante"
              mnemonicParsing="false"
              onAction="#abrirMenuProgramaEstudante"/>
   </children>
</VBox>
